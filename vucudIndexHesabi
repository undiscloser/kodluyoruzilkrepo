import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
       double kg,m,index;

       Scanner inp = new Scanner(System.in);
       System.out.print("L端tfen boyunuzu m cinsinden giriniz : ");
       m = inp.nextDouble();

       System.out.print("L端tfen kilonuzu giriniz : ");
       kg = inp.nextDouble();

       index = kg / (m*m);
       System.out.print("V端c端t indexisinz: " + index);



    }
}
